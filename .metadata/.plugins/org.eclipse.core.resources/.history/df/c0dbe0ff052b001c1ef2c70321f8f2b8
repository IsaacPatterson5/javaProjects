package myPrograms;

public class geniusNorms {
	public static void main(String[] args) {
	    System.out.println("Welcome, My Name Is Isaac"); //Print Welcome Isaac ;)
	    System.out.println("My Brother's Info & Cohort Community norms"); //Print Joke Question ;)
	    
	    System.out.println("| | | |"); //Marker-Top
	    System.out.println("V V V V"); //Marker-Top
	    
	    //Team Mate 1 = Jordan
	    System.out.println("Team Mate 1: Jordan"); //Print Name
	    System.out.println("I'm 16 years old"); //Print Age
	    System.out.println("My main passion is Basketball"); //Print Passions
	    System.out.println(""); //Print Space
	    
	    //Team Mate 2 = Pharrell
	    System.out.println("Team Mate 2: Pharrell"); //Print Name
	    System.out.println("I'm 14 years old"); //Print Age
	    System.out.println("My main passion is Basketball"); //Print Passions
	    System.out.println(""); //Print Space
	    
	    //Team Mate 3 = Ajani
	    System.out.println("Team Mate 3: Ajani"); //Print Name
	    System.out.println("I'm 14 years old"); //Print Age
	    System.out.println("My main passion is cooking"); //Print Passions and or Norms
	    System.out.println(""); //Print Space
	    
	    //Team Mate 4 = Isaac P.
	    System.out.println("Team Mate 4: Isaac"); //Print Name
	    System.out.println("I'm 15 years old"); //Print Age
	    System.out.println("My Passions are helping people, playing video games, etc."); //Print Passions
	    System.out.println(""); //Print Space
	    
	    //Community Norms
	    System.out.println("Community Norms");
	    System.out.println("------------------------------------------------------");
	    System.out.println("1. Ask Your Brothers For Help"); //Community Norm
	    System.out.println("2. Communicate With Everyone"); //Community Norm
	    System.out.println("3. Make Sure Everyone Grinds Hard A.K.A Stays On Track"); //Community Norm
	    System.out.println("4. Give A Helping Hand"); //Community Norm
	    System.out.println("5. No Cursing"); //Community Norm
	  }
}
