package myPrograms;

public class Genius {
	
	/*Main Method Decloration*/
	public static void main(String[] args) {
		gotNames(); //Call Genius Names
		gotJokes(); //Call Genius Jokes
		gotCommunityNorms(); //Call Genius Norms
		gotFavoriteActiviies(); //Call Genius Activities
		gotFavoriteFoods(); //Call Genius Favorite Foods
		gotGeniusFunctions();// Call Genius Functions
		
	}
	
	 /*gotNames Method Declaration */
	 public static void gotNames() {
			 
			 /* GENIUS NAMES - SAY OUR NAMES */
			 System.out.println("Our Names:");  //Print Genius Name
			 System.out.println("----------------------");  //Print Genius Name
			 System.out.println("My Name Is Ajani Sterling.");  //Print Genius Name
			 System.out.println("My Name Is Christan White.");  //Print Genius Name
			 System.out.println("My Name Is Isaac Patterson.");  //Print Genius Name
			 System.out.println("My Name Is Jordan Stein-Rice.");  //Print Genius Name
			 System.out.println("----------------------");  //Print Genius Name
			 System.out.println("");  //Extra Line
	       	 System.out.println("");  //Extra Line
			 
		 }
		 
	 }
	 
	 /*gotCommunityNorms Method Declaration */
	 public static void gotCommunityNorms() {
		/* GENIUS COMMUNITY NORMS - KEEP US ACCOUNTABLE */
		System.out.println("OUR COMMUNITY NORMS");
		System.out.println("----------------------");
		System.out.println("1. Ajani - Give a helping hand");
		System.out.println("2. Christan - N/A");
		System.out.println("3. Isaac - No Cussing 3");
		System.out.println("4. Jordan - Make sure everyone stays on track");
		System.out.println("----------------------");
		System.out.println("");  //Extra Line
        System.out.println("");  //Extra Line
			 
	 }
	 
	 /*gotFavoriteActiviies Method Declaration */
	 public static void gotFavoriteActiviies() {
		/* GENIUS FAVORITE ACTIVITIES - WHAT WE LIKE TO DO */
		System.out.println("OUR COMMUNITY NORMS");
		System.out.println("----------------------");
		System.out.println("1. Playing basketball");
		System.out.println("2. Playing Football");
		System.out.println("3. Play Games and Doing Math");
		System.out.println("4. Playing Video Games and helping people out.");
		System.out.println("5. Community Norm 5");
		System.out.println("----------------------");
		System.out.println("");  //Extra Line
        System.out.println("");  //Extra Line
			 
	 }
	 
	 /*gotCommunityNorms Method Declaration */
	 public static void gotFavoriteFoods() {
		/* GENIUS FAVORITE FOODS - WHAT WE LIKE TO EAT */
		System.out.println("OUR COMMUNITY NORMS");
		System.out.println("----------------------");
		System.out.println("1. Ajani - Pasta");
		System.out.println("2. Christan - Cheeseburgers");
		System.out.println("3. Isaac - Garlic Fries/Noodles");
		System.out.println("4. Jordan - Chicken");
		System.out.println("----------------------");
		System.out.println("");  //Extra Line
        System.out.println("");  //Extra Line
			 
	 }
	 
	 /*gotFavoriteActiviies Method Declaration */
	 public static void gotGeniusFunctions() {
		/* GENIUS Properties - WHAT WE LIKE TO DO */
		System.out.println("Our Properties");
		System.out.println("----------------------");
		System.out.println("1. Ajani: Eye color = brown, hair color = black");
		System.out.println("2. Christan: Eye color = brown, hair color = black");
		System.out.println("3. Isaac: Eye color = brown, hair color = black");
		System.out.println("4. Jordan: Eye color = brown, hair color = black");
		System.out.println("----------------------");
		System.out.println("");  //Extra Line
        System.out.println("");  //Extra Line
			 
	 }

}
